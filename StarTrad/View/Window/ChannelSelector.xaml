<Window x:Class="StarTrad.View.Window.ChannelSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StarTrad.View.Window"
        mc:Ignorable="d"
        Title="Installer la traduction sur..."
        Width="250" Height="183"
        MinWidth="200" MinHeight="150"
        MaxWidth="400"
        WindowStartupLocation="CenterScreen" WindowStyle="ToolWindow">
	<Grid>
		<ScrollViewer Margin="0,0,0,33">
			<ItemsControl x:Name="ItemsControl_Channels" />
		</ScrollViewer>
		<Button x:Name="Button_Ok" Content="Installer" Height="33" VerticalAlignment="Bottom" Click="Button_Ok_Click"/>
	</Grid>
</Window>
