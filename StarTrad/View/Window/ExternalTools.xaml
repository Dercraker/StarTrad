        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		x:Class="StarTrad.View.Window.ExternalTools"
        Title="ExternalTools"
		Height="247" Width="470"
		WindowStartupLocation="CenterOwner"
		ResizeMode="NoResize">
	<Grid>
		<ListBox x:Name="ListBox_Executables" SelectionMode="Single" Margin="10,10,10,35"/>

		<Button Content="Ajouter un executable externe" Margin="10,0,240,10" Height="20" VerticalAlignment="Bottom" Click="Button_Add_Click"/>
		<Button Content="Enregistrer" Margin="235,0,10,10" Height="20" VerticalAlignment="Bottom" Click="Button_Save_Click"/>
	</Grid>
</Window>
