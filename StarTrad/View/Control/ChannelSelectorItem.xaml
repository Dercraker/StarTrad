<UserControl x:Class="StarTrad.View.Control.ChannelSelectorItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StarTrad.View.Control"
             mc:Ignorable="d" 
             d:DesignHeight="50" Width="200">
	<Grid>
		<CheckBox x:Name="CheckBox_Selected" HorizontalAlignment="Left" Margin="10,14,0,10" Width="20" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" RenderTransformOrigin="0.5,0.5" IsChecked="True" />
		<Label x:Name="Label_ChannelName" Content="TECH-PREVIEW" Margin="35,5,10,5" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="14" Foreground="#FFCFDEE8"/>
	</Grid>
</UserControl>
